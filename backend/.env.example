# Django Configuration
SECRET_KEY='your-secret-key-here'  # Genera uno nuevo con: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"

DEBUG=True

# Supabase Database Configuration (Session Pooler - IPv4 compatible)
# Obtén estas credenciales en: Supabase Dashboard → Settings → Database → Connection Pooling → Session mode
DATABASE_URL=postgresql://aaaa:[YOUR_PASSWORD]@aws-1-us-east-1.pooler.supabase.com:5432/postgres

# Database individual variables (usar las mismas credenciales del pooler)
MYSQL_DATABASE=aaa
MYSQL_USER=aaaa
MYSQL_PASSWORD='your-database-password-here'
MYSQL_HOST=aaaa
MYSQL_PORT=5432
